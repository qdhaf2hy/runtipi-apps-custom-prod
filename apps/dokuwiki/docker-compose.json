{
  "services": [
    {
      "name": "dokuwiki",
      "image": "lscr.io/linuxserver/dokuwiki:2025-05-14-ls268",
      "isMain": true,
      "internalPort": 80,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/config",
          "containerPath": "/config",
          "readOnly": false
        }
      ],
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "healthCheck": {
        "test": "curl --fail http://localhost || exit 1",
        "retries": 3,
        "interval": "30s",
        "timeout": "10s"
      }
    }
  ],
  "schemaVersion": 2,
  "$schema": "../dynamic-compose-schema.json"
}
